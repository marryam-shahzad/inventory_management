<%= link_to "Go to Products", products_path %>
<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Show</th>
    </tr>
  </thead>
  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.name %></td>
        <td><%= store.location %></td>
        <td><%= link_to "Show", store_path(store) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Store", new_store_path %>
<%= link_to "Back to Products", products_path %>
